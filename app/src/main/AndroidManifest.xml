<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="peaceinfotech.malegaonbazar">


    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartUI.LaunchActivity"></activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".StartUI.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartUI.LoginActivity" />
        <activity android:name=".Signup.SignUpActivity" />
        <activity android:name=".StartUI.SelectionActivity"/>
        <activity android:name=".Signup.RegisterActivity" />
        <activity android:name=".StartUI.ChangePasswordActivity"/>
        <activity android:name=".StartUI.PasswordOTPVerification"/>
        <activity
            android:name=".User.UI.UserActivity"
            android:label="@string/title_activity_user"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme" />
        <activity android:name=".User.UI.SearchLocation" />
        <activity android:name=".User.UI.DirectionActivity" />
        <activity android:name=".User.UI.OfferInformationActivity" />
        <activity
            android:name=".User.NotificationActivity"
            android:taskAffinity=""
            android:excludeFromRecents="true">
        </activity>
        <activity
            android:name=".Vendor.UI.VendorActivity"
            android:label="@string/title_activity_vendor"
            android:theme="@style/AppTheme" />
        <activity android:name=".Vendor.UI.EditOfferActivity" />
        <activity android:name=".Vendor.UI.DemoActcivity" />
        <activity android:name=".Vendor.UI.EditServiceActivity"/>
        <activity android:name=".Vendor.UI.EditProfileActivity"/>
        <activity android:name=".Vendor.UI.AddOfferActvity"/>
    </application>

</manifest>